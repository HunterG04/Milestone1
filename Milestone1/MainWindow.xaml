<Window x:Class="Milestone1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Milestone1"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="300">
    <Grid Margin="0,0,2,0">
        <ComboBox x:Name="comboState" HorizontalAlignment="Left" Margin="73,39,0,0" VerticalAlignment="Top" Width="138" SelectionChanged="comboState_SelectionChanged"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="73,66,0,0" VerticalAlignment="Top" Width="138"/>
        <Label x:Name="labelState" Content="State" HorizontalAlignment="Left" Margin="24,39,0,0" VerticalAlignment="Top" Height="22" FontSize="10"/>
        <Label x:Name="labelCity" Content="City" HorizontalAlignment="Left" Margin="24,66,0,0" VerticalAlignment="Top" Height="22" FontSize="10"/>
        <DataGrid x:Name="dataGridBusiness" HorizontalAlignment="Left" Margin="24,129,0,0" VerticalAlignment="Top" Width="245" Height="230"/>

    </Grid>
</Window>
